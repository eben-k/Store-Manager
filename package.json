{
  "name": "store-manager",
  "version": "1.0.0",
  "description": "Help store owners manage sales and product inventory records. Single store use.",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "start:dev": "nodemon --exec npm run babel-node -- server/app.js",
    "test": "nyc mocha --compilers js:babel-register server/test --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel server -d dist",
    "babel-node": "babel-node --presets=env",
    "serve": "node dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eben-k/Store-Manager.git"
  },
  "author": "Eben",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eben-k/Store-Manager/issues"
  },
  "homepage": "https://github.com/eben-k/Store-Manager#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.1.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4"
  }
